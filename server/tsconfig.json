{
  "compilerOptions": {
    "baseUrl": "./",
    //   "types": ["vitest/globals"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "src/lib/types/*.d.ts"],
  "exclude": ["node_modules"]
}
